{
  "name": "qr-code-stencil-component",
  "version": "0.0.4",
  "description": "Stencil QRCode Component",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0",
    "qrcode-generator": "1.4.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.19",
    "@stencil/sass": "2.0.4",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack4": "6.5.16",
    "@storybook/html": "6.5.16",
    "@storybook/manager-webpack4": "6.5.16",
    "@storybook/testing-library": "0.2.1",
    "@types/jest": "^27.0.3",
    "babel-loader": "8.3.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "puppeteer": "19.11.1",
    "workbox-build": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doppelganger9/stencil-qrcode-component.git"
  },
  "author": "David LACOURT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doppelganger9/stencil-qrcode-component"
  },
  "homepage": "https://github.com/doppelganger9/stencil-qrcode-component",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js",
      "^.+\\.mdx$": "@storybook/addon-docs/jest-transform-mdx"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
